                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
	p.ID_pedido, 
	UPPER(p.estado) AS estado,
	ISNULL(p.comentarios, 'Sin Comentarios') AS comentarios,
	dp.cantidad, 
	dp.ID_producto as id_producto, 
	p.ID_cliente as id_cliente,
	t.tiempo_id as id_tiemo,
	c.ID_empleado_rep_ventas as id_empleado,
	e.ID_oficina as id_oficina,
	ps.precio_venta
FROM Pedido_ST p
INNER JOIN Detalle_Pedido_ST dp
ON dp.ID_pedido = p.ID_pedido_o
INNER JOIN Producto_ST ps 
ON dp.ID_producto = ps.ID_producto_o
INNER JOIN Cliente_ST c
ON c.ID_cliente_o= p.ID_cliente
INNER JOIN TiempoDIM t
ON p.fecha_pedido = t.fecha_pedido
INNER JOIN EmpleadoST e
ON c.ID_empleado_rep_ventas = e.ID_empleado_o</property>
